CREATE TABLE foodData (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    quantity INTEGER NOT NULL
);

selectAll:
SELECT *
FROM foodData
ORDER BY name;

insertOrReplace:
INSERT OR REPLACE INTO foodData
VALUES (?, ?, ?);

selectByName:
SELECT *
FROM foodData
WHERE name = ?;

selectById:
SELECT *
FROM foodData
WHERE id = ?;

deleteAll:
DELETE FROM foodData;

deleteByName:
DELETE
FROM foodData
WHERE name = ?;

deleteById:
DELETE
FROM foodData
WHERE id = ?;



